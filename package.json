{
  "name": "second-hand",
  "version": "1.0.0",
  "description": "Just another minimalist timer app widget",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ second-hand --platform=win32 --arch=x64 --icon=img/clock-white-128.ico --out=./build",
    "build!": "electron-packager ./ second-hand --platform=win32 --arch=x64 --icon=img/clock-white-128.ico --out=./build --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vicksonzero/second-hand.git"
  },
  "keywords": [
    "timer",
    "clock",
    "chronograph",
    "count down",
    "widget"
  ],
  "author": "Dickson Chui (vicksonzero)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vicksonzero/second-hand/issues"
  },
  "homepage": "https://github.com/vicksonzero/second-hand",
  "devDependencies": {
    "electron-prebuilt": "^1.2.0"
  },
  "dependencies": {
    "js-yaml": "^3.6.1",
    "moment": "^2.14.1",
    "node-schedule": "^1.1.1",
    "number-to-words": "^1.2.3"
  }
}
